import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static String merge(String str,int len)
    {
      char[] parts=str.toCharArray();
      int count=0;
      for(int i=0;i<len;i++)
      {
          if(parts[i]==' ')
          {
           count++;   
          }
      }
     
      int size= len-1 + (2*count);
      for(int i=len-1;i>=0;i--)
      {
            if(parts[i]==' ')   
           {
               parts[size]='%';
               parts[size-1]='2';
               parts[size-2]='0';
               size=size-3;

           }
            else if(parts[i]!=' ')   
           {
               parts[size]=parts[i];
               size--;

           }
       
      }
      System.out.println(parts);
     return new String(parts);   
    }
    
    public static void main(String[] args) throws IOException {
        InputStreamReader in=new InputStreamReader(System.in);
        BufferedReader in1=new BufferedReader(in);
        String n1=in1.readLine();
        String s=merge(n1,13);
    }
}