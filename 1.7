import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void set0(int[][] ar,int r,int c)
    {
        int[] rar= new int[r];
        int[] car= new int[c];
        
        for(int i=0;i<r;i++)
        {
            for(int j=0;j<c;j++)
            {
                if(ar[i][j]==0)
                {
                  rar[i]=1;
                  car[j]=1;
                }

            }
        }
       for(int i=0;i<rar.length;i++)
        {
            if(rar[i]==1)
            {
                 for(int j=0;j<c;j++)
                    { ar[i][j]=0;
                    }
            } 
        } 
        for(int i=0;i<car.length;i++)
        {
            if(car[i]==1)
            {
                 for(int j=0;j<r;j++)
                    { ar[j][i]=0;
                    }
            } 
        }
        
        for(int i=0;i<r;i++)
        {
            
            for(int j=0;j<c;j++)
            {
               System.out.print(ar[i][j]+" ");
            }
           System.out.println(); 
        }
       
    }   
    public static void main(String[] args) throws IOException {
        InputStreamReader in=new InputStreamReader(System.in);
        BufferedReader in1=new BufferedReader(in);
        int m=Integer.parseInt(in1.readLine());
        int n=Integer.parseInt(in1.readLine());
        int[][] ar=new int[m][n];
        for(int i=0;i<m;i++)
        {
            String[] n1=in1.readLine().split(" ");
            for(int j=0;j<n;j++)
            {
               ar[i][j]=Integer.parseInt(n1[j]); 
              
            }
            
        }
        
        set0(ar,m,n);
    }
}